%h3
  #{link_to(@league.name, league_path(@league.id))} - #{link_to(@event.name, event_path(@event.id))}
- @people.each_with_index do |person, index|
  %table
    %tr
      %td{class: 'ordinal'}= index
      %td{class: "text-left"}=link_to(person.name, person_path(person.id))
      %td{class: "text-left weight"}=person.up_by(@event)
  %br
